    "mikey179/vfsstream": "~1.6.7",
    "php-coveralls/php-coveralls": "~2.1.0",
    "phpmd/phpmd": "~2.7.0",
    "phpstan/phpstan": "~0.11.2",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "~4.1.0",
